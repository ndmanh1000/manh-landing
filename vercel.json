{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/favicon.svg",
            "dest": "/favicon.svg"
        },
        {
            "src": "/site.webmanifest",
            "dest": "/site.webmanifest"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}